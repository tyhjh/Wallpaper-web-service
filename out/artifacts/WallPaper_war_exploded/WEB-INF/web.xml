<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>wallpapers</servlet-name>
        <servlet-class>serlvet.Wallpapers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wallpapers</servlet-name>
        <url-pattern>/wallpapers</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>userInfo</servlet-name>
        <servlet-class>serlvet.UserInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userInfo</servlet-name>
        <url-pattern>/userInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>callBack</servlet-name>
        <servlet-class>serlvet.GetCallBack</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>callBack</servlet-name>
        <url-pattern>/callBack</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>update</servlet-name>
        <servlet-class>serlvet.CheckUpdate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>serlvet.Home</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>leftNote</servlet-name>
        <servlet-class>serlvet.LeftNote</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>leftNote</servlet-name>
        <url-pattern>/leftNote</url-pattern>
    </servlet-mapping>



</web-app>